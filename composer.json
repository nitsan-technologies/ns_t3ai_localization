{
    "name": "nitsan/ns-t3ai-localization",
    "type": "typo3-cms-extension",
    "description": "",
    "authors": [],
    "license": "GPL-2.0-or-later",
    "keywords": [
        "TYPO3",
        "T3AI"
    ],
    "support": {
        "issues": "https://t3planet.com/support"
    },
    "require": {
        "typo3/cms-core": "^11.5",
        "nitsan/ns-t3ai": ">=1.0.0"
    },
    "autoload": {
        "psr-4": {
            "NITSAN\\NsT3AiLocalization\\": "Classes"
        }
    },
    "replace": {
        "typo3-ter/ns-t3ai-localization": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/public",
            "extension-key": "ns_t3ai_localization"
        }
    }
}